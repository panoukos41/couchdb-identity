{
    "_id": "_design/user",
    "views": {
        "normalized_email": {
            "map": "function (doc) {\n  if (doc.split_discriminator == 'aspnetcore.user') {\n    emit(doc.email.normalized, doc._rev);\n  }\n}"
        },
        "normalized_username": {
            "map": "function (doc) {\n  if(doc.split_discriminator == 'aspnetcore.user') {\n    emit(doc.normalized_username, doc._rev);\n  }\n}"
        },
        "normalized_role_names": {
            "map": "function (doc) {\n  if (doc.split_discriminator == 'aspnetcore.user') {\n    doc.roles.forEach(role => {\n      emit(role.normalized_name, doc._rev);\n    })\n  }\n}"
        }
    },
    "language": "javascript"
}